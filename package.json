{
  "name": "react-imgp",
  "version": "1.2.0",
  "description": "React component for preloading images",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitecrownclown/react-imgp.git"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "dev": "npm run build:esm -- -w",
    "pretest": "npm run build:cjs",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib esm"
  },
  "files": [
    "esm",
    "lib"
  ],
  "keywords": [
    "react",
    "image",
    "preload",
    "img"
  ],
  "author": "Daniel Andrei",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": ">= 16.3.x"
  }
}
