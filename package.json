{
  "name": "react-imgp",
  "version": "5.0.0",
  "description": "React component for preloading images",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitecrownclown/react-imgp.git"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "dev": "npm run build:esm -- -w",
    "pretest": "npm run build:esm",
    "test": "ava --verbose --serial",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib esm"
  },
  "files": [
    "esm",
    "lib"
  ],
  "keywords": [
    "react",
    "image",
    "preload",
    "img"
  ],
  "author": "Daniel Andrei",
  "license": "MIT",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    }
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/cli": "7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "ava": "^3.15.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "esm": "^3.2.25",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">=17.0.x"
  }
}
